(defunc fizzbuzz (n) 
    (for i (in-range 1 n) 
        (cond
            ((= 0 (% i 15))
                (print "FizzBuzz")
            )
            ((= 0 (% i 3))
                (print "Fizz")
            )
            ((= 0 (% i 5))
                (print "Buzz")
            )
            (True (print i))
        )
    )
)

;; Run the FizzBuzz game from numbers 1 to 30
(fizzbuzz 31)